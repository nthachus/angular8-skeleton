{
  "name": "ng8-skeleton",
  "version": "1.0.0",
  "description": "Angular 8 Tus-client Skeleton",
  "license": "MIT",
  "scripts": {
    "ng": "node node_modules/@angular/cli/bin/ng",
    "start": "yarn ng serve -c=es5",
    "build": "yarn ng build --prod",
    "test": "yarn ng test",
    "lint": "yarn ng lint --format=stylish",
    "e2e": "yarn ng e2e --webdriverUpdate=false",
    "chromedriver": "node node_modules/webdriver-manager/bin/webdriver-manager update --gecko false --standalone false --versions.chrome",
    "format": "node node_modules/prettier/bin-prettier.js --ignore-path .gitignore .",
    "clean": "node node_modules/rimraf/bin.js dist tmp out-tsc coverage"
  },
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@auth0/angular-jwt": "^3.0.0",
    "@ng-bootstrap/ng-bootstrap": "^5.1.0",
    "@ngx-translate/core": "^12.0.0",
    "@ngx-translate/http-loader": "^5.0.0",
    "bootstrap": "~4.3.1",
    "bootswatch": "~4.3.1",
    "classlist.js": "^1.1.20150312",
    "font-awesome": "~4.7.0",
    "hash.js": "^1.0.3",
    "lodash": "^4.17.15",
    "ngx-uploadx": "^4.0.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.29",
    "@angular/cli": "~8.3.29",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@prettier/plugin-xml": "^0.12.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.136",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "~1.4.0",
    "karma-mocha-reporter": "^2.0.0",
    "prettier": "^2.0.5",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  },
  "resolutions": {
    "**/build-angular/regenerator-runtime": "^0.13.4",
    "**/color-convert/color-name": "~1.1.4",
    "**/compression/bytes": "3.1.0",
    "**/engine.io/cookie": "0.4.0",
    "**/http-errors/setprototypeof": "1.1.1",
    "prettier": "nthachus/prettier#release-2.1.2-0"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "IE 9-11"
  ],
  "prettier": {
    "trailingComma": "none",
    "arrowParens": "avoid",
    "vueIndentScriptAndStyle": true,
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "parser": "json-stringify"
        }
      }
    ]
  }
}
